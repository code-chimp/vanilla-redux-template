{
  "plugins": ["react-hooks", "jsx-a11y"],
  "extends": ["react-app", "react-app/jest", "plugin:jsx-a11y/recommended"],
  "rules": {
    "no-magic-numbers": [2, { "ignore": [-1, 0, 1, 2, 10, 100, 1000] }]
  },
  "globals": {
    "module": true,
    "console": true,
    "beforeAll": true,
    "afterAll": true,
    "window": true,
    "jest": true
  }
}
